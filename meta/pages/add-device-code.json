{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Add Device Code"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "User Code",
                              "component": "UserCode",
                              "name": "UserCodeLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "UserCode",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${devicecode.UserCode}"
                            },
                            {
                              "type": "required-validator",
                              "name": "UserCodeRequiredValidator",
                              "component": "UserCode",
                              "text": "UserCode is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Device Code",
                              "component": "DeviceCode1",
                              "name": "DeviceCode1Label",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "DeviceCode1",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${devicecode.DeviceCode1}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Subject Id",
                              "component": "SubjectId",
                              "name": "SubjectIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "SubjectId",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${devicecode.SubjectId}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Client Id",
                              "component": "ClientId",
                              "name": "ClientIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "ClientId",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${devicecode.ClientId}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ClientIdRequiredValidator",
                              "component": "ClientId",
                              "text": "ClientId is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Creation Time",
                              "component": "CreationTime",
                              "name": "CreationTimeLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "dd/MM/yyyy",
                              "type": "datepicker",
                              "name": "CreationTime",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${devicecode.CreationTime}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CreationTimeRequiredValidator",
                              "component": "CreationTime",
                              "text": "CreationTime is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Expiration",
                              "component": "Expiration",
                              "name": "ExpirationLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "dd/MM/yyyy",
                              "type": "datepicker",
                              "name": "Expiration",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${devicecode.Expiration}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ExpirationRequiredValidator",
                              "component": "Expiration",
                              "text": "Expiration is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Data",
                              "component": "Data",
                              "name": "DataLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Data",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${devicecode.Data}"
                            },
                            {
                              "type": "required-validator",
                              "name": "DataRequiredValidator",
                              "component": "Data",
                              "text": "Data is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "path": "device-codes",
                                    "type": "navigate"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new DeviceCode!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "authenticationconn.createDeviceCode",
                        "parameters": [
                          {
                            "name": "DeviceCode",
                            "value": "${devicecode}"
                          }
                        ],
                        "then": [
                          {
                            "path": "device-codes",
                            "type": "navigate"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${devicecode}",
                  "visible": "${devicecode != null}",
                  "itemType": "Testauth.Models.Authenticationconn.DeviceCode"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "devicecode",
        "type": "set",
        "value": "new Testauth.Models.Authenticationconn.DeviceCode(){}",
        "valueType": "Testauth.Models.Authenticationconn.DeviceCode"
      }
    ]
  },
  "name": "Add Device Code",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}