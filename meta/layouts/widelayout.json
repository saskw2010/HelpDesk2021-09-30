                      },
                      {
                        "code": "var query = $\"?culture={Uri.EscapeDataString((string)args)}&redirectUri={redirect}\";",
                        "type": "execute"
                      },
                      {
                        "code": "UriHelper.NavigateTo(\"Culture/SetCulture\" + query, forceLoad: true);",
                        "type": "execute"
                      }
                    ]
                  },
